        <div class="spotting"><p>An image2 <div class="num">0</div></p></div>
        <div class="spotting"><p>Another image2 <div class="num">0</div></p></div>
        <div class="spotting"><p>And another image2 <div class="num">0</div></p></div>
    </div>
     <form action="/logout" method="post">
            <input type="submit" value="Log out"/>
    </form>
</div>
<div id="testImageHere"/>
